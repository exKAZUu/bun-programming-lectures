{
  "name": "bun-programming-lectures",
  "private": true,
  "repository": "github:exKAZUu/bun-programming-lectures",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "check-all-for-ai": "bun run check-for-ai && bun run test --silent",
    "check-for-ai": "bun install > /dev/null && bun run cleanup && bun run typecheck",
    "cleanup": "bun --bun wb lint --fix --format",
    "format": "bun --bun wb lint --format",
    "lint": "bun --bun wb lint",
    "lint-fix": "bun --bun wb lint --fix",
    "prepare": "lefthook install || true",
    "test": "bun wb test",
    "typecheck": "bun --bun wb typecheck"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@openai/agents": "0.1.10",
    "openai": "6.5.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@types/bun": "1.3.0",
    "@types/micromatch": "4.0.9",
    "@willbooster/biome-config": "3.2.3",
    "@willbooster/prettier-config": "10.2.1",
    "@willbooster/wb": "10.5.1",
    "lefthook": "2.0.0",
    "prettier": "3.6.2",
    "prettier-plugin-java": "2.7.6",
    "sort-package-json": "3.4.0",
    "typescript": "5.9.3"
  }
}
